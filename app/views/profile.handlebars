<main>
    <div class="container">
        <div class="row" id="active-favors">
            <div class="col s5">
                <img id="profilepicture" src="assets/images/goodvibes.jpg" width="90%">
            </div>
            <div class="col s6">
                <div class="row"></div>
                <form>
                    <div class="usernameclass"></div>
                    <h3 id="username">Karma Friend: </h3>
                    <div class="koinsclass"></div>
                    <h3 id="koins">Koins: </h3>
                    <div class="favorsaskedclass"></div>
                    <h3 id="favorsasked">Favors Asked:</h3>
                </form>
                <ul>
                    <lh id="favor">{{#each activeFavor}}
                        <li class="activeFavors">
                            <p class="label"> {{this.favor_name}} </p>
                            <p class="label"> {{this.favor_price}} </p>
                            <p class="label"> {{this.favor_datetime}} </p>
                        </li>
                    </lh>
                    {{/each}}
                </ul>
            </div>
        </div>
        <div class="col s1"></div>
        <div class="row" id="ask-favors">
            <div class="col s1"></div>
            <div class="col s10">
                <h3 style="color:#42A5F5">Recently Asked</h3>
                <div id="mostrecentaskedfavor"></div>
                <hr>
            </div>
            <div class="col s1"></div>
        </div>
        <div class="row" id="ask-favors">
            <div class="col s1"></div>
            <div class="col s10">
                <h3 style="color:#42A5F5">Recently Completed</h3>
                <div id="mostrecentcompletedfavor"></div>
                <hr>
            </div>
            <div class="col s1"></div>
        </div>

    </div>

    </div>
